#화살표 함수로 변경하기
function ask ( question, yes, no) {
   if(confirm(question)) yes() 
   else no();
}
let ask (
“동의하십니까?”
 () =>{ alert(“동의하셨습니다”); },
 () = >{ alert(“취소 버튼을 누르셨습니다.”); }
 
